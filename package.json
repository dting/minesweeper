{
  "name": "minesweeper",
  "version": "1.0.0",
  "description": "React Redux ImmutableJS minesweeper",
  "scripts": {
    "build": "npm run clean && NODE_ENV=production webpack -p --config ./webpack.config.js --progress --profile --colors",
